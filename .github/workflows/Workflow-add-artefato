name: Helloworld(Atividade 2)

on:
  push:
    branches: [ main ]
    
env:
  WORKFLOW_VADER: 'WORKFLOW ADD ARTEFATO'

jobs:
  build:
    runs-on: ubuntu-latest
    env:
      NODE_VERSION: 12
      JOB_VADER: 'job do Artefato'
      
    steps:
    
    - name: Checkout
      uses: actions/checkout@v2.3.4
    
    - name: Upload do artefato
      uses: actions/upload-artifact@v2.2.3
      with:
        name: app-do-Artefato
        path: .
        if-no-files-found: error
        retention-days: 1
